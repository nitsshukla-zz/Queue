<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
      http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping>
        <class-a>com.navi.app.model.QueuePayload</class-a>
        <class-b>com.navi.app.dtos.Message</class-b>
        <field>
            <a>id</a>
            <b>id</b>
        </field>
        <field>
            <a>payload</a>
            <b>message</b>
        </field>
        <field>
            <a>requestId</a>
            <b>requestId</b>
        </field>
    </mapping>
    <mapping>
        <class-a>com.navi.app.model.CallbackInfo</class-a>
        <class-b>com.navi.app.dtos.CallbackDetails</class-b>
        <field>
            <a>method</a>
            <b>method</b>
        </field>
        <field>
            <a>endpoint</a>
            <b>endpoint</b>
        </field>
        <field>
            <a>headers</a>
            <b>headers</b>
        </field>
    </mapping>
    <mapping>
        <class-a>com.navi.app.model.Subscriber</class-a>
        <class-b>com.navi.app.dtos.SubscriberInfo</class-b>
        <field>
            <a>id</a>
            <b>id</b>
        </field>
        <field>
            <a>name</a>
            <b>name</b>
        </field>
        <field>
            <a>queue.name</a>
            <b>queueName</b>
        </field>
        <field>
            <a>callbackInfo</a>
            <b>callbackDetails</b>
        </field>
    </mapping>
    <mapping>
        <class-a>com.navi.app.model.Subscriber</class-a>
        <class-b>com.navi.app.dtos.SubscribeRequest</class-b>
        <field>
            <a>name</a>
            <b>name</b>
        </field>
        <field>
            <a>queue.name</a>
            <b>queueName</b>
        </field>
        <field>
            <a>callbackInfo</a>
            <b>callbackDetails</b>
        </field>
    </mapping>
</mappings>